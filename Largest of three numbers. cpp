
//task 3(Largest of three numbers)
#include <stdio.h>
int main() {
 int a,b,c;
 scanf("%d %d %d",&a,&b,&c);
 if(a>b && a>c)
 printf("Largest Number is %d",a);
 else if(b>c && b>a)
 printf("Largest Number is %d",b);
 else if(c>a && c>b)
 printf("Largest Number is %d",c);
 else
 printf("Not a valid input!!");
return 0;
}
